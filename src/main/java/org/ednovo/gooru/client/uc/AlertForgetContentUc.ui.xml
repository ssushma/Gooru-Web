<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
xmlns:gg="urn:import:org.ednovo.gooru.client.uc" xmlns:panel="urn:import:org.ednovo.gooru.client.ui">
<ui:with type="org.ednovo.gooru.client.uc.ForgetPopUpCBundle" field="res" />
<ui:with type="org.ednovo.gooru.client.mvp.home.LoginPopUpCBundle"	field="res1" />
<g:HTMLPanel styleName="{res1.css.PopupMainV2Small}">
  <div class="{res.css.forgetPopupInnerdiv}">
    <div class="{res.css.forgetPopupHeaderBg}">
      <g:Label styleName="{res.css.forgetPopupHeaderTitle}" ui:field="forgetPopupHeaderText"></g:Label>
      <div class="{res.css.forgetPopupCloseBtnContainer}">
        <panel:HTMLEventPanel ui:field="cancelButton" styleName="{res.css.forgetPopupBtnSprite} {res.css.forgetPopupCloseBtn}"></panel:HTMLEventPanel>
      </div>
    </div>
    <g:Label styleName="{res.css.loginPopupErrorMsg}" ui:field="errorMessage"/>
    <g:Label styleName="{res.css.loginPopupMessage}" ui:field="suggestionMessage"/>
    <g:HTMLPanel styleName="{res.css.forgetPopupGplusButtonContainer}">
      <panel:HTMLEventPanel ui:field="gmailButton" styleName="{res.css.forgetPopupGplusButton}">
        <g:HTMLPanel styleName="{res.css.forgetPopupGplusOuterdiv}">
          <g:HTMLPanel styleName="{res.css.forgetPopupBtnSprite} {res.css.forgetPopupGplusBtn}"></g:HTMLPanel>
        </g:HTMLPanel>
        <g:HTMLPanel styleName="{res.css.forgetPopupGplusDesc}" ui:field="signinGoogleText"></g:HTMLPanel>
      </panel:HTMLEventPanel>
    </g:HTMLPanel>
    <div class="{res.css.forgetPopupCheckBoxContainer}">
      <div class="{res.css.forgetPopupCheckBoxControl}">
        </div>
	       <div class="{res.css.forgetPopupCheckBoxDescText}"></div>
	       <g:InlineLabel styleName="{res.css.forgetPopupCheckBoxDescText}" ui:field="questionEmailText" /> <g:Anchor styleName="{res.css.supportMailID}"  target="_blank" ui:field="supportText"></g:Anchor>
    </div>
    <div class="{res.css.clear}"></div>
  </div>
</g:HTMLPanel>
</ui:UiBinder>