<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
	<ui:with type="org.ednovo.gooru.client.uc.UcCBundle" field="res" />
	<ui:with type="org.ednovo.gooru.client.mvp.home.LoginPopUpCBundle"
		field="res1" />
	<g:HTMLPanel styleName="{res1.css.PopupMainVSmall}">
	    <g:HTMLPanel styleName="{res1.css.popupInnerGrey}">
	    <g:HTMLPanel styleName="{res1.css.popupgreyHeader}">
	        <g:HTMLPanel styleName="row">
	          <g:HTMLPanel styleName="col-md-8 col-xs-8" ui:field="standardsText"></g:HTMLPanel>    
	      	  <g:HTMLPanel styleName="col-md-4 col-xs-4 {res1.css.closeContainer}">
				<g:Label ui:field="cancelButton"	styleName="{res1.css.closeButton}"></g:Label>
			 </g:HTMLPanel>
			 <g:HTMLPanel styleName="clearfix"/>
			</g:HTMLPanel>
		</g:HTMLPanel>
		<g:ScrollPanel ui:field="spanelStandardsPanel" styleName="{res.css.standardsContent}">
			<g:HTMLPanel ui:field="mainHtmlPanel">
			</g:HTMLPanel>
		</g:ScrollPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 