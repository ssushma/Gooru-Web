<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:panel="urn:import:org.ednovo.gooru.client.ui"
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>
<ui:style field="Settings" src="Setting-page.css" type="org.ednovo.gooru.client.mvp.settings.UserSettingStyle"/>

<ui:with type="org.ednovo.gooru.client.mvp.home.LoginPopUpCBundle"
		field="res" />

<g:HTMLPanel styleName="{res.css.PopupMainVVSmall}"> 
<g:HTMLPanel styleName="{res.css.popupInnerGrey}">         
<g:HTMLPanel styleName="{Settings.content}">
<g:HTMLPanel styleName="{Settings.settingsSuccessPopUpContainer}">
	<g:HTMLPanel styleName="{Settings.settingsSuccessHeaderTitleDiv}">
    <!-- <g:HTMLPanel styleName="{Settings.successHeaderText}">Success!</g:HTMLPanel> -->
      <g:HTMLPanel styleName="{Settings.successHeaderText}" ui:field="confirmEmail"></g:HTMLPanel>
    </g:HTMLPanel>
	<g:HTMLPanel styleName="{Settings.successMessageDiv}">
	   <!--  <g:HTMLPanel styleName="{Settings.settingsSuccessText}">You get a gold star for updating your profile information.</g:HTMLPanel> -->
		<g:HTMLPanel styleName="{Settings.settingsSuccessText}" ui:field="checkMailLbl"></g:HTMLPanel>
	</g:HTMLPanel>
	
	<g:HTMLPanel styleName="{Settings.successFormButtonDiv}">
		<g:Label styleName="{Settings.settingsSucessOkButton}" ui:field="lbOk"></g:Label>
	</g:HTMLPanel>
</g:HTMLPanel>
</g:HTMLPanel>       </g:HTMLPanel>
</g:HTMLPanel>
</ui:UiBinder>