<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="org.ednovo.gooru.client.mvp.authentication.SignUpCBundle"
		field="res" />
	<ui:with type="org.ednovo.gooru.client.mvp.home.LoginPopUpCBundle"
		field="res1" />
		<g:HTMLPanel styleName="{res1.css.LoginpopupMain}">
		<g:HTMLPanel styleName="{res1.css.LoginpopupInner}"  ui:field="panelPopupInner">
			<g:HTMLPanel styleName="{res1.css.LoginpopupHeader}">
				<g:HTMLPanel styleName="row">
					<g:HTMLPanel styleName="col-md-8 col-xs-8">
						<g:Label  ui:field="lblLoginHeading"></g:Label>
					</g:HTMLPanel>
					<g:HTMLPanel styleName="col-md-4 col-xs-4 {res1.css.closeContainer}">
						<g:Anchor styleName="{res1.css.closeButton}" ui:field="lblClose"></g:Anchor>
					</g:HTMLPanel>
					<g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
				</g:HTMLPanel>
			   </g:HTMLPanel>	
			
			<g:HTMLPanel styleName="col-md-12 col-xs-12 {res.css.paddingClear}">
				<g:HTMLPanel styleName="{res.css.popupContent}">
					<g:Label styleName="{res.css.congratsHeader}" ui:field="lblCongratsHeader"/>
					<g:Label styleName="{res.css.subheader}" ui:field="lblCheckYourEmail"/>
					<g:Button styleName="primary {res.css.startUsingGooru}" ui:field="btnStartUsingGooru"/>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
				<g:HTMLPanel styleName="col-md-12 col-xs-12 {res.css.footer}" ui:field="panelFooter">
					<g:Label styleName="{res.css.h2}" ui:field="lblWhatsNext"/>
					<g:Label ui:field="lblLearnHowTo"/>
					<g:HTMLPanel styleName="{res.css.DOTSbuttons}">
					    <g:Button styleName="primary {res.css.createStudentAccount}" ui:field="btnStartCreatingStudent"></g:Button>
						<!-- <g:HTMLPanel styleName="{res.css.discover} {res.css.tileContainer}">
							<g:Button styleName="{res.css.tile}" ui:field="btnDiscover"></g:Button>
							<g:Label styleName="{res.css.title}" ui:field="lblDiscover"/>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="{res.css.organize} {res.css.tileContainer}">
							<g:Button styleName="{res.css.tile}" ui:field="btnOrganize"></g:Button>
							<g:Label styleName="{res.css.title}" ui:field="lblOrganize"/>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="{res.css.teach} {res.css.tileContainer}">
							<g:Button styleName="{res.css.tile}" ui:field="btnTeach"></g:Button>
							<g:Label styleName="{res.css.title}" ui:field="lblTeach"/>
						</g:HTMLPanel> -->
					</g:HTMLPanel>
					<g:HTML styleName="{res.css.supportMessage}" ui:field="htmlSupport"/>
				</g:HTMLPanel>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>