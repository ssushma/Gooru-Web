<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.uc" xmlns:panel="urn:import:org.ednovo.gooru.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
   <ui:with type="org.ednovo.gooru.client.mvp.shelf.collection.tab.resource.add.SearchSuggestedResultWrapperCBundle" field="res" />
		<g:HTMLPanel ui:field="suggestedWrapperPanel" styleName="{res.css.searchPanel}">
		 <g:HTMLPanel styleName="{res.css.addSuggestedResourceOk}" ui:field="addResourceBtnPanel">
	          <gg:BlueButtonUc styleName="primary" ui:field="addResourceBtnLbl"/>
          </g:HTMLPanel>
		<g:FlowPanel styleName="{res.css.ratingWidgetPanel}" ui:field="ratingWidgetPanel"></g:FlowPanel>
		<g:FlowPanel ui:field="contentPanel" styleName="{res.css.contentPanel}">
			<g:FlowPanel styleName="{res.css.resourcePanel}">
				<g:FlowPanel styleName="{res.css.resourceHeaderPanel}" ui:field="resourceHeaderPanel">
					<uc:ResourceImageUc ui:field="resourceImageUc" />
					<g:FlowPanel styleName="{res.css.resourceHeaderTextPanel}" ui:field="resourceTitlePanel">
						<panel:HTMLEventPanel styleName="resourceSearchTitle" ui:field="resourceTitleContainer">
						    <g:HTML styleName="{res.css.resourceTitle}" ui:field="lblResourceTitle"></g:HTML>
						    <g:Image  styleName="{res.css.imgHeight}" ui:field="imgNotFriendly"></g:Image>
						</panel:HTMLEventPanel>
						<g:FlowPanel styleName="{res.css.metaDataPanel}" ui:field="metaDataFloPanel"/>
						<g:FlowPanel styleName="{res.css.standards}" ui:field="standardsFloPanel" />
					</g:FlowPanel>
				</g:FlowPanel>
				 <g:HTML styleName="{res.css.resourceDescription}" ui:field="resourceDescriptionHtml" />
		  <g:HTMLPanel styleName="{res.css.suggestedResourceButtonContainer}" ui:field="buttonsPanel">

        </g:HTMLPanel>
			</g:FlowPanel>
			</g:FlowPanel>
			</g:HTMLPanel>

</ui:UiBinder>