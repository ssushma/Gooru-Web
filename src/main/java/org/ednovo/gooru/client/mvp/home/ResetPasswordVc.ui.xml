<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
	<ui:with type="org.ednovo.gooru.client.mvp.home.HomeCBundle" field="res" />
		<ui:with type="org.ednovo.gooru.client.mvp.home.LoginPopUpCBundle"
		field="res1" />
	<g:FlowPanel styleName="{res.css.resetPwdContentDiv}">
		<g:Label styleName="{res.css.resetPwdTextContainer}" ui:field="newPasswordText"/>
		<g:FlowPanel styleName="{res.css.resetPwdPanel}">
			<g:Label styleName="{res.css.resetPwdLabel}" ui:field="newPwdLbl"/>
			<g:TextBox styleName="form-control {res1.css.form_control}" ui:field="resetNewPwdTxtBox"/>
			<gg:ErrorLabelUc ui:field="newPwdValidationUc"/>
		</g:FlowPanel>
		<g:FlowPanel styleName="{res.css.resetPwdPanel}">
			<g:Label styleName="{res.css.resetPwdLabel}" ui:field="confirmPwdLbl"/>
			<g:TextBox styleName="form-control {res1.css.form_control}" ui:field="resetConfirmPwdTxtBox"/>
			<gg:ErrorLabelUc ui:field="confirmPwdValidationUc"/>
		</g:FlowPanel>
		<g:FlowPanel styleName="{res.css.errorMessageContainer}">
			<g:Label styleName="{res.css.errorMessageStyle}" ui:field="tokenExpireErrorLabel"></g:Label>
		</g:FlowPanel>
		<g:FlowPanel styleName="{res.css.forgotPwdFormButtonDiv}">
			<gg:BlueButtonUc ui:field="sendMailBtnUc" />
			<g:Anchor ui:field="resetPwdCancelAnr" styleName="{res.css.forgotPwdFormCancelButton}" />
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 