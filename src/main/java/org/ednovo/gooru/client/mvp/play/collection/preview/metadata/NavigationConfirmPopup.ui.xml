<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:my="urn:import:org.ednovo.gooru.player.collection.client.ui"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:col="urn:import:org.ednovo.gooru.client.mvp.shelf.collection"
	xmlns:uc="urn:import:org.ednovo.gooru.client.uc" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
	<ui:with type="org.ednovo.gooru.client.mvp.classpages.assignments.AddAssignmentContainerCBundle" field="res"></ui:with>
	<ui:with type="org.ednovo.gooru.client.mvp.home.LoginPopUpCBundle"
		field="res1" />	
		<g:FlowPanel styleName="{res1.css.PopupMainVSmall}">
		    <g:HTMLPanel styleName="{res1.css.popupInnerGrey}">
			<g:HTMLPanel styleName="{res1.css.popupgreyHeader}">
			    <g:HTMLPanel styleName="row">
			        <g:Label styleName="col-md-12 col-xs-12" ui:field="popupHeader"></g:Label>
			     </g:HTMLPanel>
			     <g:HTMLPanel styleName="clearfix"/>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{res.css.assignmentMainContainer}">
				<g:HTMLPanel styleName="{res.css.assignmentContentContainer}">
					<g:HTMLPanel styleName="{res.css.assignmentControlsContainer}">
						<g:Label styleName="{res.css.assignmentTitleLabelForOe}" ui:field="popupText"></g:Label>
					</g:HTMLPanel>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{res.css.navigateButtonContainer}">
					<g:Button styleName="secondary"  ui:field="continueButton" />
					<g:Button styleName="primary {res.css.primaryButton}"  ui:field="backToResponseButton" />
				</g:HTMLPanel>
		</g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="clearfix"/>
	</g:FlowPanel>
</ui:UiBinder> 
