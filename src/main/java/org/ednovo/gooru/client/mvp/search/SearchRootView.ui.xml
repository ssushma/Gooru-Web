			background-image: url(images/resourcesIco.png), linear-gradient(to bottom, #EFEFEF, #FAFAFA); /* W3C */
			background-repeat:no-repeat;
			border: 1px solid #b0b0ba;
			font-size:12px;
			font-weight: bold;
			background-position:0 0;
			cursor:pointer;
			box-shadow:0 1px 3px #d0d0d5 inset;
		}
		.collectionBtnActive {
