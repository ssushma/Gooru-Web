<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.ui">
	<ui:with field="i18n" type="org.ednovo.gooru.shared.i18n.MessageProperties" />
	<ui:style field="style" src="../ProfilePageLibraryStyle.css" type="org.ednovo.gooru.client.mvp.profilepage.data.ProfilePageLibraryStyleBundle" />
	      <g:HTMLPanel styleName="{style.topicBlock}" ui:field="topicBlock">
	         <g:Label styleName="{style.topicTitle} {style.folderLevel} {style.open}" ui:field="topicTitleLbl"/>
	         <g:Label styleName="{style.libraryTopicTitle}" ui:field="libraryTopicLbl"/>
	           <panel:HTMLEventPanel styleName="{style.searchLink}" ui:field="searchLink">
	               <g:HTMLPanel styleName="{style.hoverText}" ui:field="moreOnTopicText"></g:HTMLPanel>
	               <g:HTMLPanel styleName="{style.searchMagnify}"/>
	            </panel:HTMLEventPanel>
	            <g:Button styleName="secondary" ui:field="viewAllBtn" text="{i18n.GL1946}"
				title="{i18n.GL1946}"></g:Button>
	         <g:ScrollPanel styleName="{style.conceptList}" ui:field="lessonScrollPanel">
	             <g:HTMLPanel ui:field="conceptList"/>
	         </g:ScrollPanel>
	         <g:HTMLPanel styleName="loadingImage {style.loadingIcon}" ui:field="loadingImage"></g:HTMLPanel>
	         <g:HTMLPanel styleName="{style.collectionViewer}" ui:field="collectionViewer">
	            <g:HTMLPanel styleName="{style.collectionInfo}" ui:field="collectionInfo">
	               <g:Image styleName="{style.collectionImage}" ui:field="collectionImage"></g:Image>
	               <g:HTMLPanel styleName="{style.collectionMetaData}">
		               <g:HTML styleName="{style.collectionTitlePPP}" ui:field="collectionTitleLbl"/>
		               <g:HTML styleName="{style.collectionDescription}" ui:field="collectionDescriptionLbl"/>
					   <g:FlowPanel styleName="{style.standards}" ui:field="standardsFloPanel" />
		               <g:Button styleName="secondary" ui:field="assignCollectionBtn"></g:Button>
		               <g:Button styleName="secondary" ui:field="customizeCollectionBtn"></g:Button>
	               </g:HTMLPanel>
	            </g:HTMLPanel>
	            <g:HTMLPanel styleName="{style.resourcesInside}" ui:field="resourcesInside"/>
	            <g:Label ui:field="noCollectionLbl" styleName="{style.noCollectionLbl}"></g:Label>
	         </g:HTMLPanel>
	      </g:HTMLPanel>
</ui:UiBinder> 