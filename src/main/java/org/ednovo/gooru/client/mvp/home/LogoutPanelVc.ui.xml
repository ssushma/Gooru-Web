<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	
	<g:HTMLPanel ui:field="mainContainer">
		<g:FlowPanel ui:field="logPanel">
			<uc:UlPanel
				styleName="dropdown-menu dropdowDisplay displayRight">
				<uc:LiPanel>
					<g:Anchor styleName="logoutText" ui:field="anrSettings"></g:Anchor>
				</uc:LiPanel>
				<uc:LiPanel>
					<g:Anchor styleName="logoutText" ui:field="classicGooruAnr"></g:Anchor>
					<g:HTMLPanel styleName="userMenuBottomLine" />
				</uc:LiPanel>
				<uc:LiPanel>
					<g:Anchor styleName="logoutText" ui:field="gooruTutorialsAnr"
						target="_blank"></g:Anchor>
				</uc:LiPanel>
				<uc:LiPanel>
					<g:Anchor styleName="logoutText" ui:field="supportAnr"
						target="_blank"></g:Anchor>
				</uc:LiPanel>
				<uc:LiPanel>
					<g:Anchor styleName="logoutText" ui:field="feedbackAnr"></g:Anchor>
					<g:HTMLPanel styleName="userMenuBottomLine" />
				</uc:LiPanel>
				<uc:LiPanel>
					<g:Anchor styleName="logoutText" ui:field="logoutAnr"></g:Anchor>
				</uc:LiPanel>
			</uc:UlPanel>
			<!-- <g:Anchor styleName="{res.css.logoutText}" ui:field="gooruAnr"></g:Anchor> -->

		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 