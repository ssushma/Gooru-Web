<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:htmlevents="urn:import:org.ednovo.gooru.client.ui">
	<ui:style field="playerStyle" src="resourceplayermetadata.css"
		type="org.ednovo.gooru.client.mvp.play.resource.body.ResourcePlayerMetadataBundle" />
	<ui:style field="flagPopUpstyle" src="poor-ratingflag-popup.css" />
	<ui:with field='flagBundleCss'
		type='org.ednovo.gooru.player.collection.client.view.add.tooltip.FlagBundle' />
	<g:HTMLPanel>
		<g:HTMLPanel styleName="{playerStyle.ratingPoorConfirmationContainer}">
			<g:HTMLPanel styleName="{playerStyle.arrowBorder} hidden-xs"></g:HTMLPanel>
			<g:HTMLPanel styleName="{playerStyle.arrow} hidden-xs"></g:HTMLPanel>
			<g:Label ui:field="poorRatingHeaderLbl" styleName ="{playerStyle.ratingPoorHeader}"></g:Label>
			<g:Label ui:field="poorRatingSubHeaderLbl" styleName ="{flagPopUpstyle.ratingPoorSubHeader}"></g:Label>
			<g:HTMLPanel styleName="{flagPopUpstyle.playerflagradiobutton}">
				<g:CheckBox ui:field="resourceCheckBox3"
					styleName="{flagPopUpstyle.ckeckBoxPlyerStyle}"/>
				<g:Label styleName=" {flagPopUpstyle.playerflagcontent}"
					ui:field="incorporateresourceText"></g:Label>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{flagPopUpstyle.playerflagradiobutton}">
				<g:CheckBox ui:field="resourceCheckBox2"
					styleName="{flagPopUpstyle.ckeckBoxPlyerStyle}" />
				<g:Label styleName=" {flagPopUpstyle.playerflagcontent}"
					ui:field="unavailableresourceText"></g:Label>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{flagPopUpstyle.playerflagradiobutton}">
				<g:CheckBox ui:field="resourceCheckBox1"
					styleName="{flagPopUpstyle.ckeckBoxPlyerStyle}" />
				<g:Label styleName=" {flagPopUpstyle.playerflagcontent}"
					ui:field="inaccurateTextresource"></g:Label>
			</g:HTMLPanel>


			<div class="{flagPopUpstyle.playerflagradiobutton}">
				<g:CheckBox ui:field="resourceCheckBox4"
					styleName="{flagPopUpstyle.ckeckBoxPlyerStyle}" />
				<g:Label styleName="{flagPopUpstyle.playerflagcontent}"
					ui:field="otherReason"></g:Label>
			</div>
			<g:Label styleName="{flagPopUpstyle.reviewTxtareaTitle}" ui:field="reviewTextAreaTitle"></g:Label>
			<g:TextArea styleName="{flagPopUpstyle.poorRatingTxtarea}" ui:field="ratingCommentTxtArea"></g:TextArea>
			<g:Label ui:field="mandatoryDescLblForSwareWords"></g:Label>
			<g:Label styleName="{playerStyle.errorLblBottomText}" ui:field="errorLbl"></g:Label>
			<g:HTMLPanel styleName="{playerStyle.ratingPoorSaveCancelContainer}"
				ui:field="buttonsContainer">
				<g:Button ui:field="btnSkip" styleName="secondary"></g:Button>
				<g:Button ui:field="btnPost" styleName="primary {playerStyle.btnSpace}"></g:Button>
			</g:HTMLPanel>
			<g:Label styleName="{playerStyle.savePostLbl}" ui:field="saveAndPsotLbl"></g:Label>
		</g:HTMLPanel>


	</g:HTMLPanel>

</ui:UiBinder>