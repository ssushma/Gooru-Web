<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.player.resource.client.ui">
	<ui:style field="flagPopUpstyle" src="player-flag-popup.css"/>
	<g:HTMLPanel>
<div class="{flagPopUpstyle.playerflagpopupwrapper}">
	<div class="{flagPopUpstyle.playerflagpopupinnerwrapper}">
    	<div class="{flagPopUpstyle.playerflagpopupheader}">
        	<g:Label styleName="{flagPopUpstyle.playerflagpopupheadertitle}" ui:field="flagText"></g:Label>
        	<panel:HTMLEventPanel ui:field="closeButton" styleName="{flagPopUpstyle.playerflagpopupheaderCloseBtn}">
        	    <g:Image ui:field="popUpCloseButton"/>
        	</panel:HTMLEventPanel> 
           </div>
        <div class="{flagPopUpstyle.playerflagcontentcontainer}">
            <g:HTML styleName="{flagPopUpstyle.playerflagcontentTitle}" ui:field="titleText"/>
        <!-- 	<div class="{flagPopUpstyle.playerflagcontenttext}">Flag this resource! </div> -->
        <div class="{flagPopUpstyle.playerflagradiobutton}">
            	<g:CheckBox ui:field="checkBox3" styleName="{flagPopUpstyle.ckeckBoxPlyerStyle}"/> <g:Label styleName=" {flagPopUpstyle.playerflagcontent}" ui:field="inappropriateText"></g:Label>
            </div>
             <div class="{flagPopUpstyle.playerflagradiobutton}">
            	<g:CheckBox ui:field="checkBox2" styleName="{flagPopUpstyle.ckeckBoxPlyerStyle}"/> <g:Label styleName=" {flagPopUpstyle.playerflagcontent}" ui:field="unavailableText"></g:Label>
            </div>
            <div class="{flagPopUpstyle.playerflagradiobutton}">
            	<g:CheckBox ui:field="checkBox1" styleName="{flagPopUpstyle.ckeckBoxPlyerStyle}"/> <g:Label styleName=" {flagPopUpstyle.playerflagcontent}" ui:field="inaccurateText"></g:Label>
            </div>
               
            <div class="{flagPopUpstyle.playerflagradiobutton}">
            	<g:CheckBox ui:field="checkBox4" styleName="{flagPopUpstyle.ckeckBoxPlyerStyle}"/><g:Label styleName="{flagPopUpstyle.playerflagcontent}" ui:field="otherReasonText"></g:Label>
            </div>
            <g:Label styleName="{flagPopUpstyle.playerflagcontentTitle}" ui:field="provideMoreText"></g:Label>
        <g:TextArea styleName="{flagPopUpstyle.playerflagtextarea}" ui:field="descriptionTextArea"/>
	        <div class="{flagPopUpstyle.playerflagbuttoncontainer}">
	            <g:Button styleName="{flagPopUpstyle.playerflagbuttoncancel}"  ui:field="cancelButton"/>
	             <g:Button styleName="{flagPopUpstyle.playerflagbuttonsubmit}" ui:field="submitButton"/>
	              <g:Button styleName="{flagPopUpstyle.playerflagbuttoncancelGray}" ui:field="submitButtonGray"/>
                 </div>
    </div>
</div>
</div>
	</g:HTMLPanel>
</ui:UiBinder> 