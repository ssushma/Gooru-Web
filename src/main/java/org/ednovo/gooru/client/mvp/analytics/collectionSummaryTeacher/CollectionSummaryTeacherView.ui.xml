<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field="unitStyle" src="collectionSummaryTeacher.css" />
	<ui:with field="i18n" type="org.ednovo.gooru.shared.i18n.MessageProperties"/>
	<g:HTMLPanel styleName="{unitStyle.reportsTableContainer}">
		<g:HTMLPanel ui:field="tabContainer" />
		<g:HTMLPanel ui:field="hiddenChartPnl" />
		 <g:HTMLPanel ui:field="teacherScoredDatapnl">
		 <g:HTMLPanel styleName="{unitStyle.resource_monitor_header_title}">
		      <g:HTMLPanel styleName="{unitStyle.floatLeft}"><g:InlineLabel styleName="{unitStyle.correct_legend_one}" ></g:InlineLabel><g:InlineLabel styleName="correct-one"  text="{i18n.GL2278}"></g:InlineLabel></g:HTMLPanel>
		      <g:HTMLPanel styleName="{unitStyle.floatLeft}"><g:InlineLabel styleName="{unitStyle.correct_legend_other}"></g:InlineLabel><g:InlineLabel styleName="correct-other"  text="{i18n.GL2279}"></g:InlineLabel></g:HTMLPanel>
		      <g:HTMLPanel styleName="{unitStyle.floatLeft}"><g:InlineLabel styleName="{unitStyle.incorrect_legend}"></g:InlineLabel><g:InlineLabel styleName="incorrect-ans"  text="{i18n.GL2280}"></g:InlineLabel></g:HTMLPanel>
		 </g:HTMLPanel>
		<g:HTMLPanel ui:field="teacherScoredData" styleName="{unitStyle.individualScore}"/>
		</g:HTMLPanel>
		<g:HTMLPanel ui:field="teacherOpenendedData"  styleName="{unitStyle.individualScore}"/>
		<g:HTMLPanel ui:field="teacherResourceBreakdownDatapnl">
			 <div class="{unitStyle.collectionOverViewpnl}">
			      <g:Label styleName="{unitStyle.collectionOverviewText}"  text="{i18n.GL2274}"></g:Label>
				  <div class="{unitStyle.collectionBreakDown_infobox}">
				  <g:Label styleName="{unitStyle.collectionBreakDown_infobox_titles}" text="{i18n.GL2275}"></g:Label>
				  <g:Label styleName="{unitStyle.collectionBreakDown_infobox_titles}" ui:field="totalTimeSpentlbl"></g:Label>
				  </div>
				  <div class="{unitStyle.collectionBreakDown_infobox}">
				  <g:Label styleName="{unitStyle.collectionBreakDown_infobox_titles}" text="{i18n.GL2276}"></g:Label>
				  <g:Label styleName="{unitStyle.collectionBreakDown_infobox_titles}" ui:field="totalViewlbl"></g:Label>
				  </div>
				  <div class="{unitStyle.collectionBreakDown_infobox}">
				  <g:Label styleName="{unitStyle.collectionBreakDown_infobox_titles}"  text="{i18n.GL2277}"></g:Label>
				  <g:HTMLPanel styleName="{unitStyle.collectionBreakDown_infobox_titles}" ui:field="totalAvgReactionlbl"></g:HTMLPanel> 
				  </div>
			 </div>
			 <g:ListBox ui:field="filterDropDown" styleName="{unitStyle.filterDropDown}"/>
			<g:HTMLPanel ui:field="teacherResourceBreakdownData"  styleName="{unitStyle.individualScore}" />
	 </g:HTMLPanel>
		
	</g:HTMLPanel>
</ui:UiBinder> 