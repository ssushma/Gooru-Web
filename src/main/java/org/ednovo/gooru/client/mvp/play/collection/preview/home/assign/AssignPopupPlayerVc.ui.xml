<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:a="urn:import:aj.org.objectweb.asm"
	xmlns:gt="urn:import:com.tractionsoftware.gwt.user.client.ui"  xmlns:gg="urn:import:org.ednovo.gooru.client.uc" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:with
		type="org.ednovo.gooru.client.mvp.play.collection.preview.home.assign.AssignPopUpCBundle"
		field="res" />
	<ui:with type="org.ednovo.gooru.client.mvp.home.LoginPopUpCBundle"
		field="res1" />
	<g:HTMLPanel styleName="{res1.css.PopupMainVSmall}">
		<g:HTMLPanel styleName="{res1.css.popupInnerGrey}">
			<g:HTMLPanel styleName="{res1.css.popupgreyHeader}">
			    <g:HTMLPanel styleName="row">
			    	<g:HTMLPanel styleName="col-md-8 col-xs-8">
			    		<g:Label ui:field="lblAssignTitle"/>    
			    	</g:HTMLPanel>
			    	<g:HTMLPanel styleName="col-md-4 col-xs-4 {res1.css.closeContainer}">
			    		<g:Label ui:field="cancelButton"	styleName="{res1.css.closeButton}"></g:Label>    
			    	</g:HTMLPanel>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="clearfix"/>	
			</g:HTMLPanel>
		<g:HTMLPanel styleName="{res1.css.shareSelection}">
		<g:HTMLPanel styleName="{res.css.loadingImageMainDiv}" ui:field="loadingImageLabel">
		      <g:Label  styleName="{res.css.loadingImageForSelfEdit}" />
		   </g:HTMLPanel>
	<g:HTMLPanel  ui:field="popupContentAssign">
		<g:Label ui:field="lblpopupTitle" styleName="{res1.css.dontWorrkText}"/>		  
        <g:Label styleName="{res1.css.contentDesc}" ui:field="assignDes"/>
        <g:HTMLPanel styleName="{res1.css.popupcornerBg}">
        <g:TextArea styleName="form-control {res1.css.form_control} shareTextarea" ui:field="shareLinkTxtBox">
        </g:TextArea>
      </g:HTMLPanel>
      <g:HTMLPanel styleName="{res1.css.sharenEmbedContainer}">
     	 <g:Anchor styleName="{res.css.url}" ui:field="swithUrlLbl"/>
      	<g:Anchor styleName="{res.css.url}" ui:field="swithToEmbedLbl"/>     
      </g:HTMLPanel>
     
      <g:HTMLPanel styleName="{res.css.clear}"></g:HTMLPanel>
      <g:HTMLPanel styleName="{res.css.contentBorder}"></g:HTMLPanel>
      
     <uc:HTMLEventPanel ui:field="htmlLoginPanel">   
   	    <g:Label styleName="{res.css.popupTitle}" ui:field="lblLoginPopupTitle"/>  
        <g:Label styleName="{res.css.popupDesc}" ui:field="lblAssignPopDes"/>
        <g:Button ui:field="gmailButton" styleName="{res.css.gConnectButton}"></g:Button>
        <g:HTMLPanel styleName="{res.css.divider}">
				<g:Label styleName="{res.css.dividerText}" ui:field="lblOr"/>
			</g:HTMLPanel>
    	<g:HTMLPanel styleName="{res.css.inputBlockOuter}">
       	<gg:TextBoxWithPlaceholder ui:field="loginTxtBox" setFocus="true"/>
       	<g:HTMLPanel styleName="{res.css.privacyContainer}">
			<g:InlineLabel styleName="{res.css.popupDesc}" ui:field="lblPii"></g:InlineLabel><br></br>
			<g:Anchor ui:field="ancprivacy" styleName="{res.css.privacyAlignStyle} {res.css.popupDesc}"></g:Anchor>
			<g:InlineLabel styleName="{res.css.popupDesc}" ui:field="toUsText"></g:InlineLabel>
		</g:HTMLPanel>
		<gg:TextBoxWithPlaceholder ui:field="passwordTxtBox" password="true" />
        <g:HTMLPanel styleName="{res.css.pwdTxt}"><g:Anchor ui:field="forgotPwd"></g:Anchor></g:HTMLPanel>
      </g:HTMLPanel>
      <g:HTMLPanel styleName="{res.css.btn}">
        <g:Button ui:field="loginButton" styleName="primary {res.css.LoginText}"></g:Button>
        <g:Label ui:field="lblPleaseWait" styleName="{res.css.processing}"></g:Label>
      </g:HTMLPanel>
      <g:HTMLPanel styleName="{res.css.signUp}"><g:Label ui:field="donothaveAccountText" ></g:Label>
      <g:Anchor ui:field="ancSignUp"></g:Anchor>
      </g:HTMLPanel>
    
    </uc:HTMLEventPanel>  
      
    	<uc:HTMLEventPanel ui:field="htmlEvenPanelContainer" styleName="{res.css.heightClasspagecontainer}">
			
	</uc:HTMLEventPanel>
	  
    </g:HTMLPanel>
    
	</g:HTMLPanel>
			
	</g:HTMLPanel>
	<g:HTMLPanel styleName="clearfix"/>	
	</g:HTMLPanel>

</ui:UiBinder> 