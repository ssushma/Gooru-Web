<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:header="urn:import:org.ednovo.gooru.client.mvp.play.collection.header" 
	xmlns:htmlevents="urn:import:org.ednovo.gooru.client.ui" xmlns:starRatings="urn:import:org.ednovo.gooru.client.uc"
	xmlns:html5="urn:import:org.ednovo.gooru.client.htmltags">
	
	<ui:style field="playerStyle" src="resourceplayermetadata.css" type="org.ednovo.gooru.client.mvp.play.resource.body.ResourcePlayerMetadataBundle"/>
	<ui:style field="ratingstyle" src="ratings.css"></ui:style>
	<ui:with type="org.ednovo.gooru.client.mvp.play.resource.style.PlayerStyleBundle" field="palyerStyleResource"/>
	<ui:with type="org.ednovo.gooru.shared.i18n.MessageProperties" field="i18n"></ui:with>
	<g:FlowPanel ui:field="wrapperContainerField" styleName="container {palyerStyleResource.getPlayerStyleResource.whitebg}">
		<g:FlowPanel styleName="row">
			<html5:SectionTag styleName="{palyerStyleResource.getPlayerStyleResource.blocksmall}" ui:field="collectionContainer">
				<g:FlowPanel styleName="col-md-7 col-xs-5">
					<g:FlowPanel styleName="leftBigarrow">
						<g:Button styleName="{playerStyle.spriteType} {playerStyle.next}" ui:field="backwardButton"></g:Button>
						<g:Button styleName="" ui:field="narrationButton"></g:Button>
					</g:FlowPanel>
					<g:FlowPanel styleName="{playerStyle.mainDisc}">
						<g:FlowPanel>
							<g:HTML ui:field="resourceTitleLbl" styleName="{playerStyle.resourceTitleStyle}"></g:HTML>
							<g:FlowPanel styleName="{playerStyle.collectionNormalTxt}" ui:field="resourcePublisher"></g:FlowPanel>
						</g:FlowPanel>
						<g:FlowPanel ui:field="tagsButtonContainer" styleName="{playerStyle.tagsWrapperStyle}">
							<g:FlowPanel ui:field="tagsContainer"></g:FlowPanel>
							<g:FlowPanel> <g:Button styleName="{playerStyle.plusAddButton} {playerStyle.green}" ui:field="plusAddTagsButton"></g:Button></g:FlowPanel>
						</g:FlowPanel>
					</g:FlowPanel>
				</g:FlowPanel>
				<g:FlowPanel styleName=" col-md-5 col-xs-7 {palyerStyleResource.getPlayerStyleResource.rightWrapper}">
					<g:FlowPanel styleName="rightBigarrow">
						<g:Button styleName="{playerStyle.spriteType} {playerStyle.prev}" ui:field="forwardButton"></g:Button>
					</g:FlowPanel>
					<htmlevents:HTMLEventPanel ui:field="emoticsContainer" styleName="{playerStyle.emoticsContainer}">
				    <g:HTMLPanel ui:field="singleEmoticsContainer">
						<g:HTMLPanel styleName="{playerStyle.iconOuter}">
							<g:Button styleName="{playerStyle.spriteType} {playerStyle.emoticon_ii}" ui:field="selectedEmoticButton"></g:Button>
						</g:HTMLPanel>
				   	 </g:HTMLPanel>
				   	 <g:Label styleName="{playerStyle.reactiontext}" text="{i18n.GL2100}"></g:Label>
				    <g:HTMLPanel>
							<g:HTMLPanel styleName="{playerStyle.iconOuter}">
								<g:Button  ui:field="needHelpButton" styleName="{playerStyle.spriteType} {playerStyle.emoticon_iv}"></g:Button>
								<g:HTMLPanel styleName="{playerStyle.emoticonPopup}">
									<g:HTMLPanel styleName="{playerStyle.spriteType} {playerStyle.emoticonArrow}"></g:HTMLPanel>
									<g:Label styleName="{playerStyle.emoticonPopupInner}" ui:field="reactionToolTipFive"></g:Label>
								</g:HTMLPanel>
							</g:HTMLPanel>
							<g:HTMLPanel styleName="{playerStyle.iconOuter}">
								<g:Button ui:field="doNotUnderstandEmoticButton" styleName="{playerStyle.spriteType} {playerStyle.emoticon_iii}"></g:Button>
								<g:HTMLPanel styleName="{playerStyle.emoticonPopup}">
									<g:HTMLPanel styleName="{playerStyle.spriteType} {playerStyle.emoticonArrow}"></g:HTMLPanel>
									<g:Label styleName="{playerStyle.emoticonPopupInner}" ui:field="reactionToolTipFour"> </g:Label>
								</g:HTMLPanel>
							</g:HTMLPanel>
							<g:HTMLPanel styleName="{playerStyle.iconOuter}">
								<g:Button  ui:field="mehEmoticButton" styleName="{playerStyle.spriteType} {playerStyle.emoticon_viii}"></g:Button>
								<g:HTMLPanel styleName="{playerStyle.emoticonPopup}">
									<g:HTMLPanel styleName="{playerStyle.spriteType} {playerStyle.emoticonArrow}"></g:HTMLPanel>
									<g:Label styleName="{playerStyle.emoticonPopupInner}" ui:field="reactionToolTipThree"></g:Label>
								</g:HTMLPanel>
							</g:HTMLPanel>
							<g:HTMLPanel styleName="{playerStyle.iconOuter}">
							<g:Button ui:field="understandEmoticButton" styleName="{playerStyle.spriteType} {playerStyle.emoticon_ii}"></g:Button>
								<g:HTMLPanel styleName="{playerStyle.emoticonPopup}">
									<g:HTMLPanel styleName="{playerStyle.spriteType} {playerStyle.emoticonArrow}"></g:HTMLPanel>
									<g:Label styleName="{playerStyle.emoticonPopupInner}" ui:field="reactionToolTipTwo"></g:Label>
								</g:HTMLPanel>
							</g:HTMLPanel>
							<g:HTMLPanel styleName="{playerStyle.iconOuter}">
							<g:Button ui:field="canExplainEmoticButton" styleName="{playerStyle.spriteType} {playerStyle.emoticon_i}"></g:Button>
								<g:HTMLPanel styleName="{playerStyle.emoticonPopup}">
									<g:HTMLPanel styleName="{playerStyle.spriteType} {playerStyle.emoticonArrow}"></g:HTMLPanel>
									<g:Label styleName="{playerStyle.emoticonPopupInner}" ui:field="reactionToolTipOne"></g:Label>
								</g:HTMLPanel>
						</g:HTMLPanel>
					</g:HTMLPanel>
			    </htmlevents:HTMLEventPanel>
					<g:HTMLPanel  styleName="{ratingstyle.ratingContainer}" ui:field="ratingsContainer">
					    <g:Label styleName="{playerStyle.ratingValue}" ui:field="starValue"></g:Label>
		                <g:Label styleName="{playerStyle.ratingValue}" ui:field="mouseOverStarValue"></g:Label> 
		                <g:HTMLPanel styleName="{ratingstyle.subRatingContainer} rating playerrating"> 
		                    <g:InlineHTML ui:field="one_star" styleName="ratingItems star"></g:InlineHTML>
		                    <g:InlineHTML ui:field="two_star" styleName="ratingItems star"></g:InlineHTML>
		                    <g:InlineHTML ui:field="three_star" styleName="ratingItems star"></g:InlineHTML>
		                    <g:InlineHTML ui:field="four_star" styleName="ratingItems star"></g:InlineHTML>
		                    <g:InlineHTML ui:field="five_star" styleName="ratingItems star"></g:InlineHTML>
		                </g:HTMLPanel>
	            	</g:HTMLPanel>
				</g:FlowPanel>
			</html5:SectionTag>
		</g:FlowPanel>
		<g:FlowPanel styleName="row" ui:field="rowPanel">
			<g:FlowPanel styleName="col-md-12 {palyerStyleResource.getPlayerStyleResource.bodysection}">
				<html5:SectionTag styleName="" ui:field="resourceWidgetContainer"></html5:SectionTag>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>