<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
	<ui:with type="org.ednovo.gooru.client.mvp.authentication.SignUpCBundle"
		field="res" />
	<ui:with type="org.ednovo.gooru.client.mvp.home.LoginPopUpCBundle"
		field="res1" />
	<g:HTMLPanel styleName="{res1.css.LoginpopupMain}">
		<g:HTMLPanel styleName="{res1.css.popupInnerLeaveReg}">
		    <g:HTMLPanel styleName="{res1.css.LoginpopupHeader}">
		        <g:HTMLPanel styleName="row">
		            <g:HTMLPanel styleName="col-md-9 col-xs-9">
		        		<g:Label  ui:field="lblTitle"/>        
		            </g:HTMLPanel>
		    	    <g:HTMLPanel styleName="col-md-3 col-xs-3 {res1.css.closeContainer}">
		    	    	<g:Anchor styleName="{res1.css.closeButton}" ui:field="lblCancel"></g:Anchor>    
		    	    </g:HTMLPanel> 
					<g:HTMLPanel styleName="clearfix"/>            
		        </g:HTMLPanel> 
		    </g:HTMLPanel>
			
			<g:HTMLPanel ui:field="panelSignUp">
				<g:HTMLPanel styleName="{res1.css.popupContentLeaveRegistration}">
					<g:Label styleName="{res1.css.SignUp_green_h1}"  ui:field="lblHeading" />
					<g:Label styleName="{res1.css.SignUp_h2}" ui:field="lblSubHeading" />
				</g:HTMLPanel>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{res1.css.leaveRegistrationButtonContainer}">
				<g:Button ui:field="btnLeave" styleName="secondary"></g:Button>
				<g:Button ui:field="btnContinue" styleName="primary"></g:Button>
			</g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="{res1.css.leaveRegistrationBottomContainer}">
		    	<g:HTMLPanel styleName="{res1.css.leaveRegistarionFooterText}">
			    <gg:PPanel>
			   		 <g:Label ui:field="quriesText" />
			    <g:InlineLabel ui:field="pleaseContactText"/><g:Anchor ui:field="supportLink" target="_blank"></g:Anchor>     
			    </gg:PPanel>
			   
			    </g:HTMLPanel>
			</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 