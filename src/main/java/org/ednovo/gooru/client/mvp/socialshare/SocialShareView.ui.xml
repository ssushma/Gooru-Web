<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">

	<g:HTMLPanel  ui:field="socialShareContainer">
		<g:HTMLPanel>
			<g:HTMLPanel ui:field="shareTextPanel">
					<uc:PPanel ui:field="fbPanel">
						<g:Anchor ui:field="facbookText" styleName="ssv-facebookbtn"></g:Anchor>
 					</uc:PPanel>
				<panel:HTMLEventPanel	ui:field="twitterPanel">
					<uc:PPanel  ui:field="panelTwitter">
						<g:Anchor  ui:field="twitterText" styleName="ssv-twitterbtn"></g:Anchor>
					</uc:PPanel>
				</panel:HTMLEventPanel>


				<panel:HTMLEventPanel	ui:field="emailPanel">
					<uc:PPanel  ui:field="panelEmail">
						<g:Anchor ui:field="emailText" styleName="ssv-emailbtn"></g:Anchor>
 					</uc:PPanel>
				</panel:HTMLEventPanel>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="shareIconPanel">
				<panel:HTMLEventPanel styleName="ssv-classPageShareButtonsOuterDiv"
					ui:field="fbIconPanel">
					<g:HTMLPanel styleName="ssv-fbPageShareIconButtonsBg" ui:field="panelfbIcon">
						<g:HTMLPanel
							styleName="ssv-classPageShareButtonSpriteOuterDiv ssv-classPageShareButtonSpriteAlign">
							<g:HTMLPanel
								styleName="ssv-classPageShareButtonsSprite ssv-classPageShareButtonSpriteFacebook"></g:HTMLPanel>
						</g:HTMLPanel>
					</g:HTMLPanel>
				</panel:HTMLEventPanel>

				<panel:HTMLEventPanel styleName="ssv-classPageShareButtonsOuterDiv"
					ui:field="twIconPanel">
					<g:HTMLPanel styleName="ssv-twitterPageShareIconButtonsBg"  ui:field="panelTwIcon">
						<g:HTMLPanel
							styleName="ssv-classPageShareButtonSpriteOuterDiv ssv-classPageShareButtonSpriteTAlign">
							<g:HTMLPanel
								styleName="ssv-classPageShareButtonsSprite ssv-classPageShareButtonSpriteTwitter"></g:HTMLPanel>
						</g:HTMLPanel>
					</g:HTMLPanel>
				</panel:HTMLEventPanel>


				<panel:HTMLEventPanel styleName="classPageShareButtonsOuterDiv"
					ui:field="emailIconPanel">
					<g:HTMLPanel styleName="ssv-classPageShareButtonsFTEBg"  ui:field="panelEmailIcon">
						<g:HTMLPanel
							styleName="ssv-classPageShareButtonSpriteOuterDiv ssv-classPageShareButtonSpriteEAlign">
							<g:HTMLPanel
								styleName="ssv-classPageShareButtonsSprite ssv-classPageShareButtonSpriteEmail"></g:HTMLPanel>
						</g:HTMLPanel>
					</g:HTMLPanel>
				</panel:HTMLEventPanel>

			</g:HTMLPanel>

			<g:Image ui:field="categoryImage" styleName="ssv-displayNone"></g:Image>
		</g:HTMLPanel>
	</g:HTMLPanel>

</ui:UiBinder>