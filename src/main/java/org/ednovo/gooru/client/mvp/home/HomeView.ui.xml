				<g:Label styleName="{res.css.subheader}" ui:field="lblSubHeading"
					text="{i18n.GL2047}"></g:Label>
				<g:Button styleName="primary {res.css.signUpButton}"
					ui:field="btnSignUp" text="{i18n.GL0186}"></g:Button>
			</g:HTMLPanel>
			<section class="searchContainer">
				<div class="containerMain">
					<div class="row">
						<div class="col-md-3 searchMAIN">
							<div class="sInner">
								<div class="animation text-center">
									<img src="images/landing-page/find.png" />
								</div>
								<h2 class="text-center">Find</h2>
								<div class="clearfix"></div>
								<p class="">Browse thousands of K-12 collections made by teachers
									or search over 18M resources</p>

								<tbp:TextBoxWithPlaceholder
									styleName="form-control" placeholder="e.g. Shakespeare"
									ui:field="txtSearch"></tbp:TextBoxWithPlaceholder>
								<g:Button styleName="primary" ui:field="btnSearch"
									text="{i18n.GL0176}"></g:Button>

							</div>
						</div>
						<div class="col-md-3">
							<div class="sInner">
								<div class="animation text-center">
									<img src="images/landing-page/remix.png" />
								</div>
								<h2 class="text-center">Remix</h2>
								<div class="clearfix"></div>
								<p class="">Remix collections and customize content to to meet
									your students’ needs.</p>

								<g:Button styleName="primary {res.css.btnBlock}" ui:field="btnMoreOnCollections">More on Collections</g:Button>

							</div>
						</div>
						<div class="col-md-3">
							<div class="sInner">
								<div class="animation text-center">
									<img src="images/landing-page/share.png" />
								</div>
								<h2 class="text-center">Share</h2>
								<div class="clearfix"></div>
								<p class="">Share collections with students via My Classes. Login
									is not required to access.
								</p>
								<g:Button styleName="primary {res.css.btnBlock}" ui:field="btnMoreOnClasses">More on Classes</g:Button>

							</div>
						</div>
						<div class="col-md-3">
							<div class="sInner">
								<div class="animation text-center">
									<img src="images/landing-page/monitor.png" />
								</div>
								<h2 class="text-center">Monitor</h2>

								<div class="clearfix"></div>

								<p class="">Measure your students' engagement and progress to
									intervene in real-time.</p>

								<g:Button styleName="primary {res.css.btnBlock}" ui:field="viewSampleResportsBtn">View Sample Reports</g:Button>

							</div>
						</div>
					</div>
				</div>
