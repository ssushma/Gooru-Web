		<g:HTMLPanel ui:field="gooruPanel" />
		<g:HTMLPanel ui:field="panelLandingPage">
			<g:HTMLPanel styleName="{res.css.headerPanel}">
				<g:Label styleName="{res.css.header}" ui:field="lblHeading"
