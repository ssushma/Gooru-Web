<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.uc" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
	<ui:style field="style" src="successpopup.css" />
	<ui:with type="org.ednovo.gooru.client.mvp.home.LoginPopUpCBundle"
		field="res1" />
		<g:FlowPanel styleName="{res1.css.PopupMainVSmall}">
			<g:HTMLPanel styleName="{style.popupHeader}">
				<g:Label styleName="{style.popupHeaderText}" ui:field="successPopupHeader"></g:Label>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{style.assignmentSuccessMainContainer}">
				<g:HTMLPanel styleName="{style.assignmentContentContainer}">
					<g:Label styleName="{style.successImage}"></g:Label>
					<g:HTML styleName="{style.successText}" ui:field="successPopupBodyText"></g:HTML>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{style.assignmentButtonsContainer}">
					<g:Button styleName="primary" ui:field="okayButton" />
				</g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="clearfix"/>
	</g:FlowPanel>
</ui:UiBinder>