<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

	<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:header="urn:import:org.ednovo.gooru.client.mvp.play.collection.header" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
 	<ui:style field="addToResourcesOrCollectionStyle" src="addresource.css" />
	<g:HTMLPanel styleName="{addToResourcesOrCollectionStyle.collectionPlayerAddResourcesContainer} addResourcePopup">
	<g:HTMLPanel styleName="{addToResourcesOrCollectionStyle.collectionPlayerAddResourcesContent}" ui:field="resourceImageContainerInAddResource">
    	<g:HTMLPanel styleName="{addToResourcesOrCollectionStyle.collectionPlayerAddResourcesIconContainer}">
            <g:HTMLPanel styleName="{addToResourcesOrCollectionStyle.collectionPlayerAddResourcesText}" ui:field="addresourceText"></g:HTMLPanel>
         </g:HTMLPanel>
         <g:HTMLPanel  styleName="{addToResourcesOrCollectionStyle.addCollectionContainer}" ui:field="addCollectionContainer">
	         <g:Label  styleName="{addToResourcesOrCollectionStyle.collectionPlayerAddResourcesCollectionInstead}" ui:field="addToExistingColl">
	         </g:Label>
         </g:HTMLPanel>
    </g:HTMLPanel>
    
    <g:FlowPanel styleName="{addToResourcesOrCollectionStyle.successMessageContainer}" ui:field="resourceAddedSuccessMessageContainer">
    	<g:Label styleName="{addToResourcesOrCollectionStyle.successMessageLabelText}" ui:field="successMessageLabelText"></g:Label> 
    	<g:HTMLPanel styleName="{addToResourcesOrCollectionStyle.workSpaceLinkContainer}">
    		<!-- <g:Anchor styleName="{addToResourcesOrCollectionStyle.workSpaceLinkText}" ui:field="workSpaceLink"></g:Anchor> -->
    		<g:Button styleName="secondary" ui:field="workSpaceBtn" ></g:Button>
    	</g:HTMLPanel> 
    	<g:HTMLPanel styleName="{addToResourcesOrCollectionStyle.addCollectionInsteadLabelContainer}" ui:field="addCollectionInsteadLabelContainer">
    		<g:Label styleName="{addToResourcesOrCollectionStyle.addCollectionInsteadLabelText}" ui:field="addCollectionInsteadLabelText"></g:Label>
    	</g:HTMLPanel>
    </g:FlowPanel>
    <g:HTMLPanel styleName="{addToResourcesOrCollectionStyle.collectionPlayerExistingCollectionContainer}" ui:field="existingCollectionContainer">
    	<div class="{addToResourcesOrCollectionStyle.collectionPlayerExistingCollectionContent}">
        	<!-- <div class="{addToResourcesOrCollectionStyle.collectionPlayerExistingCollectionText}" >Add to an existing collection</div>       -->
            <div class="{addToResourcesOrCollectionStyle.collectionPlayerExistingCollectionInputContainer}">
            	<g:InlineLabel ui:field="dropdownListPlaceHolder" styleName="{addToResourcesOrCollectionStyle.dropdownListPlaceHolder}"></g:InlineLabel>
            	<g:ScrollPanel ui:field="dropdownListContainerScrollPanel" styleName="{addToResourcesOrCollectionStyle.dropdownListContainerScrollPanel}">
	            	<g:HTMLPanel ui:field="dropdownListContainer" styleName="{addToResourcesOrCollectionStyle.dropdownListContainer}">
			   		 </g:HTMLPanel>
		   		 </g:ScrollPanel>
            </div>
            <g:Button styleName="{addToResourcesOrCollectionStyle.collectionPlayerExistingAddButtonText}" ui:field="addResourceToCollectionButton"></g:Button>
            <g:Label ui:field="addingLabel" styleName="{addToResourcesOrCollectionStyle.addingLabelText}"></g:Label>
            <g:Label ui:field="resourceAdditionErrorStyle" styleName="{addToResourcesOrCollectionStyle.resourceAdditionErrorStyle}"></g:Label>
        </div>
        
        	<g:HTMLPanel  styleName="{addToResourcesOrCollectionStyle.collectionPlayerNewColletionText}" ui:field="createCollectionLabelContainer">
        		<g:Label styleName="{addToResourcesOrCollectionStyle.addNewLabel}" ui:field="addNewCollectionLabel"></g:Label>
        	</g:HTMLPanel>
        	<g:Label ui:field="errorMessage" styleName="{addToResourcesOrCollectionStyle.errorLabelText}" ></g:Label>
            <g:Label ui:field="sizeMessage" styleName="{addToResourcesOrCollectionStyle.errorLabelText}" ></g:Label>
    </g:HTMLPanel>
     <uc:HTMLEventPanel styleName="{addToResourcesOrCollectionStyle.resource-info-bottomImageContainer}" ui:field="hideButton">
      <g:Label styleName="{addToResourcesOrCollectionStyle.resource-info-hideImage}"/>
      <g:Label styleName="{addToResourcesOrCollectionStyle.resource-info-hideText}" ui:field="hideText"></g:Label>
 </uc:HTMLEventPanel>
</g:HTMLPanel>
	
</ui:UiBinder> 