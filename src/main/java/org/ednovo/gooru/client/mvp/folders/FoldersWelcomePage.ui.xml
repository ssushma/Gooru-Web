<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"  xmlns:sg="urn:import:org.ednovo.gooru.client.mvp.home">
	<ui:style field="WelcomePage" src="folderwelcomepage.css" />
	<g:HTMLPanel>
		<g:HTMLPanel styleName="{WelcomePage.container} {WelcomePage.clearfix}">
			<g:HTMLPanel styleName="{WelcomePage.startsection}">
				<g:Label styleName="{WelcomePage.title}" ui:field="lblStartCreateingCollection"></g:Label>
				<g:Label styleName="{WelcomePage.subtitle}" ui:field="lblCollectionDesc"></g:Label>
				<br />
				<g:Button styleName="primary" ui:field="btnCreateCollection"></g:Button>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{WelcomePage.startsection} {WelcomePage.fright}">
				<g:HTMLPanel styleName="{WelcomePage.codeBg}">
						<g:HTMLPanel styleName="{WelcomePage.desc-emphasis}">
							<g:Label styleName="{WelcomePage.resourceImg}" ui:field="lblViewWalkthrough"></g:Label>
							<g:HTML styleName="{WelcomePage.sampleCollection}" ui:field="htmlWalkSampleCollection"></g:HTML>
							<g:HTMLPanel styleName="{WelcomePage.arrowContainer}">
								<g:Image url="images/EmptyPages/arrow.png" width="40"
									height="20" />
							</g:HTMLPanel>
						</g:HTMLPanel>	
						<g:HTMLPanel styleName="{WelcomePage.collectionContainer}">
						<g:Anchor ui:field="imgSampleCollection">
						
								</g:Anchor>
							
						</g:HTMLPanel>
				</g:HTMLPanel>
				
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{WelcomePage.clearfix}"></g:HTMLPanel>
			<g:HTMLPanel styleName="{WelcomePage.leftPanel}">
				<g:HTMLPanel styleName="{WelcomePage.navBox}">
					<g:Label styleName="{WelcomePage.h3}" ui:field="lblCollaborate"></g:Label>
					<g:Label styleName="{WelcomePage.desc}" ui:field="lblFurther"></g:Label>
					<g:HTMLPanel styleName="{WelcomePage.hoverCircle}">
						<g:Label styleName="{WelcomePage.circleTitle}" ui:field="lblOne"></g:Label>
					</g:HTMLPanel>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{WelcomePage.navBox}">
					<g:Label styleName="{WelcomePage.h3}" ui:field="lblUpload"></g:Label>
					<g:Label styleName="{WelcomePage.desc}" ui:field="lblCustomizeCollections"></g:Label>
					<g:HTMLPanel styleName="{WelcomePage.hoverCircle}">
						<g:Label styleName="{WelcomePage.circleTitle}" ui:field="lblTwo"></g:Label>
					</g:HTMLPanel>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{WelcomePage.navBox}">
					<g:Label styleName="{WelcomePage.h3}" ui:field="lblAssess"></g:Label>
					<g:Label styleName="{WelcomePage.desc}" ui:field="lblAddQuestion"></g:Label>
					<g:HTMLPanel styleName="{WelcomePage.hoverCircle}">
						<g:Label styleName="{WelcomePage.circleTitle}" ui:field="lblThree"></g:Label>
					</g:HTMLPanel>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{WelcomePage.navBox}">
					<g:Label styleName="{WelcomePage.h3}" ui:field="lblGuideWith"></g:Label>
					<g:Label styleName="{WelcomePage.desc}" ui:field="lblGuideNarration"></g:Label>
					<g:HTMLPanel styleName="{WelcomePage.hoverCircle}">
						<g:Label styleName="{WelcomePage.circleTitle}" ui:field="lblFour"></g:Label>
					</g:HTMLPanel>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{WelcomePage.navBox}">
					<g:Label styleName="{WelcomePage.h3}" ui:field="lblCustomizeResource"></g:Label>
					<g:Label styleName="{WelcomePage.desc}" ui:field="lblEditStart"></g:Label>
					<g:HTMLPanel styleName="{WelcomePage.hoverCircle}">
						<g:Label styleName="{WelcomePage.circleTitle}" ui:field="lblFive"></g:Label>
					</g:HTMLPanel>
				</g:HTMLPanel>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{WelcomePage.rightPanel}">
				<g:Image url="images/EmptyPages/view-img.png" width="667"
					height="941" />
			</g:HTMLPanel>
		</g:HTMLPanel>
		<sg:FooterUc />
	</g:HTMLPanel>
</ui:UiBinder> 