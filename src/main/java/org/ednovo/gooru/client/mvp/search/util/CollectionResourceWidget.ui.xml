<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel styleName="col-md-4 col-sm-4 visibleElement">
	<div class="panel panel-default">
		<div class="panel-heading">
			<div class="no-gutter">
				<div class="col-md-7 col-sm-8 col-xs-8 ratings">
					<div class="resultRating">
						<g:FlowPanel ui:field="ratingWidgetPanel"/>
					</div>
				</div>
				<div class="col-md-5 col-sm-4 col-xs-4 viewContainer">
				<g:Label styleName="plus">+</g:Label><g:Label ui:field="lbladdCount" styleName="authorName"></g:Label>
					<span>
						<img src="images/eye.png"/> <g:Label ui:field="lblViewCount" styleName="authorName"></g:Label>
					</span>
				</div>
				<div class="clearfix"></div>
			</div>
		</div>
		<div class="panel-body">
			<div class="no-gutter">
				<div class="col-md-6 col-sm-6 resourceImgContainer">
					<a>
						<g:Image ui:field="resourseImage" />
						<g:HTMLPanel styleName="resourceName" ui:field="imageOverlay"></g:HTMLPanel>
					</a>
				</div>
				<div class="col-md-6 col-sm-6 resourceDesc">
					<g:Label styleName="collectionTitle" ui:field="resourceTitle"/>
					<div class="resourceAuthorDetails">NASA</div>
				</div>
				<div class="clearfix"></div>
				<g:HTMLPanel styleName="resourceDescription" ui:field="resourceDescription"/>
				<div class="standerdContainer resourcesStanders">
					<span class="additionalCount"></span>
					<div class="clearfix"></div>
					<g:FlowPanel ui:field="standardsDataPanel"/>
				</div>
				<g:HTMLPanel styleName="usedDetails">
				<g:InlineLabel ui:field="lblUserCount"></g:InlineLabel>
					<g:Anchor ui:field="ancViewMore">View More</g:Anchor>
				</g:HTMLPanel>
				<div class="userDetailsResources no-gutter">
					<div class="col-md-8 col-xs-8 col-sm-8 collectionResources">
						<g:Image styleName="resourceCollectionImage" ui:field="relatedCollectionImage" />					
						<g:InlineLabel ui:field="relatedCollectionTitle" styleName="collectionTitleforResource"></g:InlineLabel>
					</div>
					<div class="col-md-4 col-xs-4 col-sm-4 user">
						<a href="#">
						<g:Image ui:field="creatorImage"/>
						</a>
					</div>
					<div class="clearfix"></div>
				</div>
				<div class="clearfix"></div>
			</div>
		</div>
		<div class="panel-footer no-gutter">
			<div class="col-md-12 col-sm-12 remixButton">
				<g:Button styleName="primary" ui:field="btnAddResource">Add</g:Button>
			</div>
			<div class="clearfix"></div>
		</div>
	</div>
</g:HTMLPanel>
</ui:UiBinder> 