	<ui:with type="org.ednovo.gooru.client.mvp.home.LoginPopUpCBundle"
		field="res" />
	
	<g:HTMLPanel styleName="{res.css.PopupMainSmall}">
		<g:HTMLPanel styleName="{res.css.popupInnerGrey}">
			<g:HTMLPanel styleName="{res.css.popupgreyHeader}">
			    <g:HTMLPanel styleName="row">
			    <g:HTMLPanel styleName="col-md-8 col-xs-8">
			    	<g:Label ui:field="headertext"></g:Label>    
			    </g:HTMLPanel>    
			 <g:HTMLPanel styleName="col-md-4 col-sm-4 {res.css.closeContainer}">
				<panel:HTMLEventPanel styleName="{res.css.closeButton}"	ui:field="closeButton"></panel:HTMLEventPanel>
			</g:HTMLPanel>
				<g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
				</g:HTMLPanel>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{res.css.whatNew}">
			<g:HTMLPanel styleName="col-md-4 col-sm-4 {res.css.whatLeft}">
				<g:Label styleName="{res.css.improveGooruDesc}" ui:field="subtext"></g:Label>
				<g:Label styleName="{res.css.improveGooruDescriptionx}" ui:field="goorutext"></g:Label>
			</g:HTMLPanel>
			
			<g:HTMLPanel styleName="col-md-8 col-sm-8 {res.css.whatRight} {res.css.borderLeft}">
				<g:Label styleName="{res.css.improveGooruSubHeading}"	ui:field="headersubtext"></g:Label>
					<!-- First -->
				<g:HTMLPanel styleName="improveGooruContneOuterDiv">
					<g:HTMLPanel styleName="improveGooruContneOuter">
						<g:Label styleName="{res.css.improveGooruBlueText}"
							ui:field="lblTitleFive"></g:Label>
						<g:InlineLabel styleName="{res.css.improveGooruBlueTextDesc}"
							ui:field="aboutFive"></g:InlineLabel>
						<g:Anchor styleName="improveGooruHyperlinkText"
							ui:field="descLinkFive" target="_blank"></g:Anchor>
					</g:HTMLPanel>
				</g:HTMLPanel>
				<!-- Second -->
				<g:HTMLPanel styleName="improveGooruContneOuterDiv">
					<g:HTMLPanel styleName="improveGooruContneOuter">
						<g:Label styleName="{res.css.improveGooruBlueText}"	ui:field="lblnavigation"></g:Label>
						<g:InlineLabel styleName="{res.css.improveGooruBlueTextDesc}"
							ui:field="aboutNavigation"></g:InlineLabel>
							<g:HTMLPanel>
						<g:Anchor styleName="improveGooruHyperlinkText"		ui:field="descLinkNavigation" target="_blank"></g:Anchor>
							</g:HTMLPanel>
					</g:HTMLPanel>
				</g:HTMLPanel>
				<!-- Third -->
				<g:HTMLPanel styleName="improveGooruContneOuterDiv">
					<g:HTMLPanel styleName="improveGooruContneOuter">
						<g:Label styleName="{res.css.improveGooruBlueText}"		ui:field="lblTitleSix"></g:Label>
						<g:InlineLabel styleName="{res.css.improveGooruBlueTextDesc}"	ui:field="aboutSix"></g:InlineLabel>
						<g:Anchor styleName="improveGooruHyperlinkText"		ui:field="descLinkSix" target="_blank"></g:Anchor>
					</g:HTMLPanel>
				</g:HTMLPanel>
				<!-- Fourth -->
				<g:HTMLPanel styleName="improveGooruContneOuterDiv">
					<g:HTMLPanel styleName="improveGooruContneOuter">
						<g:Label styleName="improveGooruBlueText"	ui:field="lblTitleSeven"></g:Label>
						<g:InlineLabel styleName="{res.css.improveGooruBlueTextDesc}"		ui:field="aboutSeven"></g:InlineLabel>
						<g:HTMLPanel>
						<g:Anchor styleName="improveGooruHyperlinkText"	ui:field="descLinkSeven" target="_blank"></g:Anchor>
					</g:HTMLPanel>
					</g:HTMLPanel>
				</g:HTMLPanel>
			 </g:HTMLPanel>
			 </g:HTMLPanel>	
			 <g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
				<!-- <g:HTMLPanel styleName="{style.improveGooruContneOuterDiv}">
					<g:HTMLPanel styleName="{style.improveGooruContneOuter}">
						<g:Label styleName="{style.improveGooruBlueText}"
							ui:field="lblTitleEight"></g:Label>
						<g:InlineLabel styleName="{style.improveGooruBlueTextDesc}"
							ui:field="aboutEight"></g:InlineLabel>
						<g:Anchor styleName="{style.improveGooruHyperlinkText}"
							ui:field="descLinkEight" target="_blank"></g:Anchor>
					</g:HTMLPanel>
				</g:HTMLPanel> -->

				<g:HTMLPanel styleName="{res.css.improveGooruLinkDesc}"		ui:field="GooruLinkOutercontainer">
				   <g:InlineLabel ui:field="questiontxt"></g:InlineLabel>
				   <g:Anchor ui:field="lblSupportLink" 	target="_blank" styleName="improveGooruSupportLinkDesc"></g:Anchor>    
				</g:HTMLPanel>

			
			<!-- <panel:HTMLEventPanel styleName="{style.improveGooruSupportLinkDesc}" 
				ui:field="supportlinklbl">support@goorulearning.org</panel:HTMLEventPanel> -->
		</g:HTMLPanel>
