		}

		.improveGooruBlueTextDesc span {

		}

		.improveGooruLearnmore {
		cursor: pointer;
		text-decoration: none !important;
		margin-left: 15px;
		}

		.improveGooruCloseBtnContainer {
		float: right;
		padding: 10px 10px 0px 0px;
		}

		.improveGooruCloseBtnContainer:hover {
		cursor: pointer;
		}

		.improveGooruBtnSprite {
		background: url(images/sprite-new.png) no-repeat;
		}

		.improveGooruCloseBtn {
		background-position: -52px -116px;
		width: 18px;
		height: 19px;
		}

		.improveGooruLinkOuter {
			float: right; /* margin: 85px 15px 2px 5px; */
			width: 235px !important;
			margin-top: 70px;
		}

		.improveGooruLinkDesc {
		float: left;
		font-family: Arial, Helvetica, sans-serif;
		font-size: 11px;
		line-height: 120%;
		color: #515151;
		text-align: left;
		}

		.improveGooruSupportLinkDesc {
		text-decoration: none !important;
		font-family: Arial, Helvetica, sans-serif;
		font-size: 11px;
		line-height: 120%;
		text-align: left;
		padding: 0px 0px 0px 3px;
		cursor: pointer;
		float: left;
		}

		.improveGooruInnerDivContainer {
		float: left;
		display: inline-block;
		vertical-align: TOP;
		width: 420px;
		}

		.improveGooruOuterDivContainer {
		float: left;
		width: 227px;
		/* height: 498px; */
		}

		.imporoveMainContainer {
		clear: both;
		}

