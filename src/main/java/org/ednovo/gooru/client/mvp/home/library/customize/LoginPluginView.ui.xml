<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:a="urn:import:aj.org.objectweb.asm"
	xmlns:gt="urn:import:com.tractionsoftware.gwt.user.client.ui"  xmlns:gg="urn:import:org.ednovo.gooru.client.uc" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:with
		type="org.ednovo.gooru.client.mvp.home.library.assign.AssignPopUpCBundle"
		field="res" />
		<ui:with type="org.ednovo.gooru.client.mvp.home.LoginPopUpCBundle"
		field="res1" />
			<g:HTMLPanel>
	      	  <g:HTMLPanel styleName="{res1.css.dontWorrkText}" ui:field="hangOnText"></g:HTMLPanel>
		      <g:Label ui:field="collectionDescription" styleName="{res1.css.contentDesc}"></g:Label>
		      <g:HTMLPanel styleName="{res.css.loginPopupStyle}">
			    <g:Label styleName="{res.css.loginWithGooru}" ui:field="lblLoginwithGooru"></g:Label>
			    <g:Button ui:field="gmailButton" styleName="{res1.css.gConnectButton}"></g:Button>
			    <g:HTMLPanel styleName="{res.css.dividerCustomize}">
			      <g:Label styleName="{res1.css.dividerText}" ui:field="lblOr"/>
			  </g:HTMLPanel>
		      <g:HTMLPanel styleName="{res.css.inputBlockOuter}">
		        <gg:TextBoxWithPlaceholder ui:field="loginTxtBox" setFocus="true" styleName="form-control {res1.css.form_control}"/>

				<gg:TextBoxWithPlaceholder ui:field="passwordTxtBox" password="true"  styleName="form-control {res1.css.form_control}"/>
				
		        <g:HTMLPanel styleName="{res1.css.forgotPasswordLink}">
		            <g:Anchor ui:field="forgotPwd"></g:Anchor>
		        </g:HTMLPanel>
		      </g:HTMLPanel>
		      <g:HTMLPanel styleName="{res1.css.loginPopupbtnContainer}">
		        <g:Button ui:field="loginButton" styleName="primary"></g:Button>
		        <g:Label ui:field="lblPleaseWait" styleName="{res.css.processing}"></g:Label>
		      </g:HTMLPanel>
		      <g:HTMLPanel styleName="{res.css.privacyCustomizeContainernew}">
					<g:InlineLabel ui:field="lblPii"></g:InlineLabel>
					<g:Anchor ui:field="ancprivacy" styleName="{res.css.privacyAlignStyleNew}"></g:Anchor>
					<g:InlineLabel ui:field="toUsText"></g:InlineLabel>
				</g:HTMLPanel>
		      </g:HTMLPanel>
		      <g:HTMLPanel styleName="{res1.css.haveAccountContainer}" ui:field="signUpPanel">
		          <g:HTMLPanel styleName="{res1.css.haveAccount}">
		        	  <g:InlineLabel ui:field="donotHaveAcount"></g:InlineLabel>
		          	  <g:Anchor ui:field="ancSignUp"></g:Anchor>      
		          </g:HTMLPanel>
		     </g:HTMLPanel>
			</g:HTMLPanel>
</ui:UiBinder> 