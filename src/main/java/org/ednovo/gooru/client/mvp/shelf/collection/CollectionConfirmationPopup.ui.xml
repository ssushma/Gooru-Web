<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
	<ui:style>
		.shareAlertPopup {
			font-family: Arial;
			font-size: 12px;
			width: 434px;
			border: 1px solid #ccc;
		}
		.PopupMainVSmall{
			width:550px;
			color:#515151;
			margin: 30px auto;
			position:relative;
			font-family:arial;
			font-size:12px;
			background:#ffffff;
			border-radius:0 0;
			border:1px solid #999;
			padding:3px 3px;
			z-index:999;
		}
		.alertHeaderStyle {
			background: -webkit-gradient(linear, 0 0, 0 100%, from(#666), to(#515151));
			background: -webkit-linear-gradient(top, #666, #515151);
			background: -moz-linear-gradient(top, #666, #515151);
			background: -ms-linear-gradient(top, #666, #515151);
			background: -o-linear-gradient(top, #666, #515151);
			background-color: #515151;
			color: #fff;
			padding: 10px 15px;
		}
		.alertHeaderTxtStyle {
			font-size: 16px;
			color: #fff;
		}
		.alertBodyStyle {
			padding: 23px 30px 15px 30px;
		}
		.alertSuccessTxt {
			padding: 10px;
			text-align: center;
			font-size: 12px;
		}
		.alertSuccessTitleTxt {
			padding: 10px;
			font-weight: 100;
			font-size: 16px;
			text-align: center;
			margin-bottom: 10px;
		}
		.alertSuccessMsgTxt {
			
		}
		.okSuccessBtn {
			width: 386px;
			padding-top: 21px;
		}
		.resourceHeaderPanel {
			float: left;
			width: 375px;
			margin: 20px 10px;
			height: 125px;
			overflow-y: scroll;
		}
		.privateMsgTxt {
			padding: 10px;
			text-align: center;
			border-bottom: 1px solid #ccc;
		}
		.alertHeaderCloseStyle{
		    width: 17px;
			height: 17px;
			background: url(images/login/close-button.png) no-repeat;
			position: absolute;
			top: 18px;
			right: 14px;
			cursor:pointer;

	      }
	
	</ui:style>
	<g:HTMLPanel styleName="{style.PopupMainVSmall}" >
	   <g:HTMLPanel styleName="{style.alertHeaderStyle}">
	      <g:Label styleName="{style.alertHeaderTxtStyle}" ui:field="shareMsgTitle"/>
	       <g:Label styleName="{style.alertHeaderCloseStyle}" ui:field="closeButton"/>
	   </g:HTMLPanel>
	      <g:HTMLPanel styleName="{style.alertBodyStyle}">
	         <g:HTMLPanel styleName="{style.alertSuccessMsgTxt}">
	         
	         </g:HTMLPanel> 
	         <g:HTMLPanel styleName="{style.alertSuccessTitleTxt}">
	            <g:Label styleName="{style.alertSuccessMsgTxt}" ui:field="shareMsgTxt"/>
	            <g:Label styleName="{style.alertSuccessMsgTxt}" ui:field="classPageNames"/>
	         </g:HTMLPanel>
	         <g:HTMLPanel styleName="{style.alertSuccessTxt}" >
	            <g:Label styleName="{style.alertSuccessMsgTxt}" ui:field="shareMsgLbl"/>
	         </g:HTMLPanel>
	         <g:HTMLPanel styleName="{style.okSuccessBtn}" ui:field="buttonContainer">
	         	<gg:BlueButtonUc  ui:field="goBackBtn" styleName="secondary" />
	         	<gg:BlueButtonUc  ui:field="okButton" styleName="primary" />
	         </g:HTMLPanel>
	      </g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 