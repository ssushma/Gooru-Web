<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field="playerStyle" src="searchInfo.css" />
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more
		details */
		.standardDiv {
		padding: 10px 0px 0px 18px;
		}
		.title {
		display:
		inline-block;
		font-size: 12px;
		font-weight: bold;
		color: #515151;
		}
		.normal {
		display:
		inline-block;
		font-style: normal;
		font-size:
		12px;
		color: #515151;
		vertical-align: middle;
		}
		.standard {
		display:
		inline-block;
		vertical-align:
		top;
		color: #FFF;
		font-size: 10px;
		padding:
		3px 5px;
		margin-right: 3px;
		border-radius: 4px;
		background-color:
		#515151;
		background:
		-webkit-gradient(linear, 0% 0%, 0% 100%,
		from(#666), to(#515151));
		background: -webkit-linear-gradient(top,
		#666, #515151);
		background:
		-moz-linear-gradient(top, #666, #515151);
		background:
		-ms-linear-gradient(top, #666, #515151);
		background:
		-o-linear-gradient(top, #666, #515151);
		}

		.resource-info-stand-wrapper{
		width: 100%;
		float: left;
		font-size: 12px;
		font-family: Arial, Helvetica,
		sans-serif;
		color: #515151;
		}
	</ui:style>
	<g:HTMLPanel>
		<!-- Add GWT widgets here. Enter < and hit Ctrl-Space to auto-complete 
			widget names. The ui:field attribute binds an element to a field in the owner 
			class. See the GWT docs on UI Binder for more details. -->
		<g:HTMLPanel styleName="{playerStyle.loadingImageMainDivShare} hidden-xs"
			ui:field="loadingImagePanel">
			<g:Label styleName="{playerStyle.loadingImageForShare}" />
		</g:HTMLPanel>
		<g:Label ui:field="noInfoAvailable"></g:Label>		
		<g:HTMLPanel styleName="{style.standardDiv}" ui:field="gradesPanel">
			<g:Label styleName="{style.title}" ui:field="lblGradeTitle" />
			<g:Label styleName="{style.normal}" ui:field="gradesText" />
		</g:HTMLPanel>
		<!-- <g:HTMLPanel styleName="{style.standardDiv}"> <g:HTMLPanel styleName="{style.title}"> 
			Grade: <g:HTMLPanel styleName="{style.standard}">CCSS.ELA-Literacy.L.6.2</g:HTMLPanel> 
			<g:HTMLPanel styleName="{style.standard}">CCSS.ELA-Literacy.L.6.2</g:HTMLPanel> 
			</g:HTMLPanel> </g:HTMLPanel> -->
		<g:HTMLPanel styleName="{style.standardDiv}" ui:field="panelStandrads">
			<g:Label styleName="{style.title}" ui:field="lblStandrads" />
			<g:Label styleName="{style.normal}" ui:field="lblStandardsText"></g:Label>
			<g:FlowPanel ui:field="standardsInfoConatiner"
				styleName="{style.resource-info-stand-wrapper}" />
		</g:HTMLPanel>
		<g:HTMLPanel styleName="{style.standardDiv}" ui:field="panelDesc">
			<g:Label styleName="{style.title}" ui:field="lblLanguage"></g:Label>
			<g:Label styleName="{style.normal}" ui:field="lblLanguageText"></g:Label>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="{style.standardDiv}" ui:field="dKnowledgePanel">
			<g:Label styleName="{style.title}" ui:field="lblDepthKnowledge"></g:Label>
			<g:HTMLPanel styleName="{style.normal}" ui:field="dKnowledgeType"></g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="{style.standardDiv}" ui:field="learningSkillsMainPanel">
			<g:Label styleName="{style.title}" ui:field="lblLearningSkills"></g:Label>
			<g:HTMLPanel styleName="{style.normal}" ui:field="learningSkillsPanel"></g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="{style.standardDiv}" ui:field="panelAudience">
			<g:Label styleName="{style.title}" ui:field="lblAudience"></g:Label>
			<g:Label styleName="{style.normal}" ui:field="lblAudienceValue" />
		</g:HTMLPanel>
		<g:HTMLPanel styleName="{style.standardDiv}" ui:field="panelInstructional">
			<g:Label styleName="{style.title}" ui:field="lblInstructional"></g:Label>
			<g:Label styleName="{style.normal}" ui:field="lblInstructionalValue"></g:Label>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="{style.standardDiv}" ui:field="panelOer">
			<g:Label styleName="{style.title}" ui:field="lblOer"></g:Label>
			<g:Label styleName="{style.normal}" ui:field="lblOerValue"></g:Label>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 